    "tsc": "tsc --noEmit -w --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDesjardins/phaser3-rollup-typescript.git"
  },
  "author": "Patrick Desjardins",
  "homepage": "https://github.com/MrDesjardins/phaser3-rollup-typescript",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/node": "^20.11.30",
    "@types/webfontloader": "^1.6.38",
    "concurrently": "^8.2.2",
    "phaser": "^3.80.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.4"
  },
  "dependencies": {
    "webfontloader": "^1.6.28"
  }
