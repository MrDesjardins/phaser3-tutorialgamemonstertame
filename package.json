{
  "name": "phaser3-rollup-typescript",
  "version": "1.0.0",
  "description": "Phaser 3 TypeScript Starter",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run tsc\" \"vite\"",
